{
  "version": 2,
  "builds": [
    { "src": "src/main.ts", "use": "@vercel/node" },
    { "src": "src/main.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
